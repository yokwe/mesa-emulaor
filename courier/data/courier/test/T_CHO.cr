T_CHO: PROGRAM 0 VERSION 0 =
BEGIN

-- predefined
--   BOOLEAN, BYTE, CARDINAL, LONG_CARDINAL, STRING, UNSPECIFIED, UNSPECIFIED2, UNSPECIFIED3,
-- constructed
--   ENUM, ARRAY, SEQUENCE, RECORD, CHOICE, PROCEDURE, ERROR
-- reference
--    REFERENCE

    E: TYPE = {e(0)};
    A: TYPE = ARRAY 1 OF CARDINAL;
    S: TYPE = SEQUENCE 1 OF CARDINAL;
    R: TYPE = RECORD [ ];
    C: TYPE = CHOICE OF { a(0) => RECORD [ ] };

    CHO_A_PRE01: TYPE = CHOICE OF { a(0) => BOOLEAN };
    CHO_A_PRE02: TYPE = CHOICE OF { a(0) => BYTE };
    CHO_A_PRE03: TYPE = CHOICE OF { a(0) => CARDINAL };
    CHO_A_PRE04: TYPE = CHOICE OF { a(0) => LONG CARDINAL };
    CHO_A_PRE05: TYPE = CHOICE OF { a(0) => STRING };
    CHO_A_PRE06: TYPE = CHOICE OF { a(0) => UNSPECIFIED };
    CHO_A_PRE07: TYPE = CHOICE OF { a(0) => UNSPECIFIED2 };
    CHO_A_PRE08: TYPE = CHOICE OF { a(0) => UNSPECIFIED3 };

    CHO_A_COM01: TYPE = CHOICE OF { a(0) => ARRAY 4 OF CARDINAL };
    CHO_A_COM02: TYPE = CHOICE OF { a(0) => SEQUENCE 4 OF CARDINAL };
    CHO_A_COM03: TYPE = CHOICE OF { a(0) => RECORD[ a: CARDINAL] };
    CHO_A_COM04: TYPE = CHOICE OF { a(0) => CHOICE OF { a(0) => CARDINAL } };
    
    CHO_A_ALI01: TYPE = CHOICE OF { a(0) => E };
    CHO_A_ALI02: TYPE = CHOICE OF { a(0) => A };
    CHO_A_ALI03: TYPE = CHOICE OF { a(0) => S };
    CHO_A_ALI04: TYPE = CHOICE OF { a(0) => R };
    CHO_A_ALI05: TYPE = CHOICE OF { a(0) => C };
    
    CHO_T_PRE01: TYPE = CHOICE E OF { e => BOOLEAN };
    CHO_T_PRE02: TYPE = CHOICE E OF { e => BYTE };
    CHO_T_PRE03: TYPE = CHOICE E OF { e => CARDINAL };
    CHO_T_PRE04: TYPE = CHOICE E OF { e => LONG CARDINAL };
    CHO_T_PRE05: TYPE = CHOICE E OF { e => STRING };
    CHO_T_PRE06: TYPE = CHOICE E OF { e => UNSPECIFIED };
    CHO_T_PRE07: TYPE = CHOICE E OF { e => UNSPECIFIED2 };
    CHO_T_PRE08: TYPE = CHOICE E OF { e => UNSPECIFIED3 };

    CHO_T_COM01: TYPE = CHOICE E OF { e => ARRAY 4 OF CARDINAL };
    CHO_T_COM02: TYPE = CHOICE E OF { e => SEQUENCE 4 OF CARDINAL };
    CHO_T_COM03: TYPE = CHOICE E OF { e => RECORD[ a: CARDINAL] };

    CHO_T_ALI01: TYPE = CHOICE E OF { e => E };
    CHO_T_ALI02: TYPE = CHOICE E OF { e => A };
    CHO_T_ALI03: TYPE = CHOICE E OF { e => S };
    CHO_T_ALI04: TYPE = CHOICE E OF { e => R };
    CHO_T_ALI05: TYPE = CHOICE E OF { e => C };

END.