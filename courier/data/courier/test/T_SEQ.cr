T_SEQ: PROGRAM 0 VERSION 0 =
BEGIN

-- predefined
--   BOOLEAN, BYTE, CARDINAL, LONG_CARDINAL, STRING, UNSPECIFIED, UNSPECIFIED2, UNSPECIFIED3,
-- constructed
--   ENUM, ARRAY, SEQUENCE, RECORD, CHOICE, PROCEDURE, ERROR
-- reference
--    REFERENCE

    E: TYPE = {a(1), b(2), c(3), d(4)};
    A: TYPE = ARRAY 4 OF CARDINAL;
    S: TYPE = SEQUENCE 4 OF CARDINAL;
    R: TYPE = RECORD [ a: CARDINAL ];
    C: TYPE = CHOICE OF { a(0) => CARDINAL };
    
    SEQ_ALI01: TYPE = SEQUENCE 4 OF E;
    SEQ_ALI02: TYPE = SEQUENCE 4 OF A;
    SEQ_ALI03: TYPE = SEQUENCE 4 OF S;
    SEQ_ALI04: TYPE = SEQUENCE 4 OF R;
    SEQ_ALI05: TYPE = SEQUENCE 4 OF C;

END.